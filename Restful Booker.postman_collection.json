									"});\r",
									"\r",
									"//из сниппетов\r",
									"//проверка на наличие в ответе заголовка \"Content-Type\"\r",
									"pm.test(\"Content-Type is present\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"});\r",
									"\r",
									"//проверка на наличие в ответе заголовка \"Content-Type\" и точных его значений, добавленных через запятую\r",
									"pm.test(\"Content-Type is present\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\", \"text/plain; charset=utf-8\");\r",
									"});\r",
									"\r",
									"//проверка на наличие конкретной части \"text/plain\" в ответе заголовка \"Content-Type\"\r",
									"pm.test(\"Content-Type is present\", function () {\r",
									"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"text/plain\");\r",
									"});"
